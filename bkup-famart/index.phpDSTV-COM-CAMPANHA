  function funcao() {
   var r = confirm('Quer mesmo sair?');;

   if (r == true) {
    window.location.assign("http://www.globo.com") /* ou o seu sair.php */
  } 
  else {
    alert('Nao funcionou');
  }
  return false;
}
</script>


